<!--
This module serves as the Debuging environment and default implementation for integration with Javascript applications

-->

<module>

  <inherits name='com.google.gwt.user.User'/>
    <inherits name='com.googlecode.gwtx.Java'/>
  <inherits name='com.google.gwt.xml.XML'/>
    <inherits name="org.pentaho.gwt.widgets.Widgets"/>
  
    <inherits name="org.pentaho.ui.xul.Gwt"/>
    <source path="">
        <exclude name="**/SwingMqlEditor*"/>
        <exclude name="**/MQLEditorServiceImpl*"/>
        <exclude name="**/MQLEditorServiceDeligate*"/>
        <exclude name="**/MQLEditorGwtServlet*"/>
        <exclude name="**/MQLEditorServiceSyncImpl*"/>
        <exclude name="**/ModelSerializer*"/>
        <exclude name="**/SwingDatasourceEditor*"/>
        <exclude name="**/ConnectionServiceImpl*"/>
        <exclude name="**/DatasourceServiceImpl*"/>
        <exclude name="**/ConnectionServiceDelegate*"/>
        <exclude name="**/DatasourceServiceDelegate*"/>
        <exclude name="**/ConnectionGwtServlet*"/>
        <exclude name="**/DatasourceGwtServlet*"/>
        <exclude name="**/ConnectionServiceSyncImpl*"/>
        <exclude name="**/DatasourceServiceSyncImpl*"/>       
    </source>
    <public path="datasourceeditor/public"/>
    <entry-point class="org.pentaho.commons.metadata.mqleditor.editor.JavascriptDatasourceEditor"/>
    
  <servlet path="/ConnectionService" class="org.pentaho.commons.metadata.mqleditor.editor.service.gwt.ConnectionDebugGwtServlet" />
  <servlet path="/DatasourceService" class="org.pentaho.commons.metadata.mqleditor.editor.service.gwt.DatasourceDebugGwtServlet" />
  
 </module>